
public class node {
	protected string name;
	protected node next;
	
	node (String name){
		this.name = name;
		this.next = null;
		
	}
	public String getName() {
		return name;
		
	}
	public void setName(String name) {
		this.name = name;
	}
	public node getNext() {
		return next;
	}
	public void setNext(node next) {
		this.next = next;
	}

}
